{
  "name": "phprometheus/phprometheus-laravel",
  "description": "Opinionated library for shipping metrics to Prometheus.",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Kieran Patel",
      "email": "me@kieranajp.co.uk"
    }
  ],
  "autoload": {
    "psr-4": {
      "PhprometheusLaravel\\": "src/"
    }
  },
  "require": {
    "php": ">=7.3",
    "phprometheus/phprometheus": "^0.1",
    "laravel/framework": ">=6.0"
  },
  "require-dev": {
    "kahlan/kahlan": "^5"
  },
  "extra": {
    "laravel": {
      "providers": [
        "PhprometheusLaravel\\ServiceProvider"
      ]
    }
  }
}
